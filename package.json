  "engines": {
    "node": ">=16.14.2"
  },
  "scripts": {
    "preview": "yarn build:dev && yarn serve",
    "serve": "http-server build 3000",
    "render": "ts-node src/render.tsx",
    "start": "concurrently \"yarn start:react\" \"yarn start:functions\" \"yarn watch:server\" \"yarn watch:service-worker\"",
    "start:react": "react-scripts start",
    "build": "yarn build:server && yarn build:react && yarn render && yarn build:service-worker",
    "build:dev": "HOST=localhost:3000 PROTOCOL=\"http:\" yarn build",
    "build:react": "react-scripts build",
    "build:server": "yarn tsc -p server/tsconfig.json",
    "build:service-worker": "yarn tsc -p service-worker/tsconfig.json",
    "watch:server": "yarn tsc-watch -p server/tsconfig.json",
    "watch:service-worker": "yarn tsc-watch -p service-worker/tsconfig.json",
    "start:functions": "yarn wrangler pages dev functions --kv ROAMTUNES",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "yarn eslint -c .eslintrc.progressive.js --ext js,jsx,ts,tsx src",
    "gql:generate": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "2.3.5",
