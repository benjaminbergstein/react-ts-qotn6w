{
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["esnext", "webworker"],
    "outDir": "../public",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": ["node"],
    "jsx": "react",
    "noEmit": false,
    "allowJs": true,
    "target": "es2015"
  },
  "include": ["./**/*.ts"]
}
